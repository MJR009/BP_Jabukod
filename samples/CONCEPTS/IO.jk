// C-like

int main() {
    print("Hello world!\n");

    string x;
    scan(x);
    print("You entered ");
    print(x);
    print("\n");

    return 0;
}

/* * */

// Raku-like

int main() {
    spurt "Hello world!\n";

    string x;
    slurp x;
    spurt "You entered "
    spurt x;
    spurt "\n";

    return 0;
}

/* * */

// Analogous to above, variadic

int main() {
    spurt "Hello world!";

    string x;
    slurp x;
    spurt "You entered ", x, "\n";

    return 0;
}
